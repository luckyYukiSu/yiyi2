.body{
  width: 100%;
  padding: 30rpx;
}
.container{
  width: 100%;
  padding: 25rpx;
  min-height: 900rpx;
  /* height: 900rpx; */
  border-radius: 20rpx;
  background: #fff;
}
.container_header{
  border-bottom: 4rpx solid #ccdfd5;
  padding-bottom: 10rpx;
}
.nav-font{
  font-size: 36rpx;
}
.text-select{
  color: #1B8185;
}
.wx-canvas {
  position: relative;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}